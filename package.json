{
  "name": "@coffeekitkat/eslint-fmt-code",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.6.2",
  "description": "",
  "author": "coffeekitkat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/coffeekitkat/eslint-fmt-code.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "types": {
        "import": "./dist/nuxt.d.ts",
        "require": "./dist/nuxt.d.cts"
      },
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./dotenv": {
      "types": {
        "import": "./dist/dotenv.d.ts",
        "require": "./dist/dotenv.d.cts"
      },
      "import": "./dist/dotenv.js",
      "require": "./dist/dotenv.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "watch": "npm run build -- --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "tsx src/index.ts",
    "test": "vitest --test-timeout=24000",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@nuxt/kit": "^3.17.2",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "dotenv": "^16.5.0",
    "systeminformation": "^5.25.11"
  },
  "devDependencies": {
    "@antfu/ni": "^23.2.0",
    "@antfu/utils": "^8.1.0",
    "@types/node": "^22.10.6",
    "bumpp": "^9.10.1",
    "eslint": "^9.18.0",
    "lint-staged": "^15.3.0",
    "pnpm": "^10.6.2",
    "simple-git-hooks": "^2.11.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  }
}
